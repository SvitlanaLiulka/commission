{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb"],
  "rules": {
    "no-useless-computed-key": 0,
    "import/no-named-as-default": 0,
    "no-console": "warn",
    "no-eval": "error",
    "import/first": "error",
    "operator-linebreak": 0,
    "no-param-reassign": 0,
    "object-curly-newline": 0,
    "arrow-body-style": 0,
    "import/prefer-default-export": 0,
    "no-unused-vars": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
